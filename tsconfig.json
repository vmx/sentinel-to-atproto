{
  "compilerOptions": {
    "target": "es2024",
    "lib": ["es2024"],
    "jsx": "react-jsx",
    "module": "es2022",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["test"],
  "include": ["worker-configuration.d.ts", "src/**/*.ts"]
}
